<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="basicdialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <Countrylist
                        jcr:primaryType="cq:Widget"
                        field_x0020_Lable="select country"
                        name="./selection"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <v1
                                jcr:primaryType="cq:Widget"
                                text="india"
                                value="india"/>
                            <v2
                                jcr:primaryType="cq:Widget"
                                text="japan"
                                value="japan"/>
                            <v3
                                jcr:primaryType="cq:Widget"
                                text="china"
                                value="china"/>
                        </options>
                    </Countrylist>
                    <full_x0020_name
                        jcr:primaryType="cq:Widget"
                        fieldLable="enter the full name"
                        name="./fullname"
                        xtype="txtfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
